{
    "builds": [
      {
        "src": "server/server.js",
        "use": "@now/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server.js" },
      { "src": "/(.*)", "dest": "public/$1" }
    ]
  }